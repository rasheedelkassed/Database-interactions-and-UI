<h1>MySQL Results:</h1>
<p>{{{results}}}</p>

<table>
	<tr>
		<th>Name</th>
		<th>Reps</th> 
		<th>Weight</th>
		<th>Date</th>
		<th>Unit</th>
	</tr>
	{{#each dataList}}
    <tr>
		<td>this.name</td>
		<td>this.reps</td> 
		<td>this.weight</td>
		<td>this.date</td>
		<td>this.unit</td>
	</tr>
    {{/each}}
</table>