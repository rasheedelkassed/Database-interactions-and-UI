<h1>MySQL Results:</h1>
<table>
	<tr>
		<th>Name</th>
		<th>Reps</th> 
		<th>Weight</th>
		<th>Date</th>
		<th>Unit</th>
	</tr>
	{{#each dataList}}
    <tr>
		<input><>
		<td>{{this.name}}</td>
		<td>{{this.reps}}</td> 
		<td>{{this.weight}}</td>
		<td>{{this.date}}</td>
		<td>{{this.unit}}</td>
		<td><form>
			<input id="{{this.id}}" type="hidden" value="{{this.id}}" onclick="bindDeleteButton({{this.id}})"/>
			<input type="submit">Delete</input>
			<input type="submit">Edit</input>
		</form></td>
	</tr>
    {{/each}}
</table>